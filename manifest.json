{ "name": "LanguageBoost",
  "description": "LanguageBoost",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": [],
  "host_permissions": [],

  "content_scripts": [
    {
        "matches": ["https://www.primevideo.com/*", "https://primevideo.com/*"],
        "js": ["./background/back.js","./background/openModal.js"]
    }],
 "web_accessible_resources": [
        {
          "resources": ["./background/background.js"],
          "matches": ["https://www.primevideo.com/*", "https://primevideo.com/*"]
      }
    ],
  "action": {
    "default_title": "LanguageBoost",
    "default_popup": "./components/LanguageBoost/dist/index.html",
    
  },
  "icons": {
  }
}
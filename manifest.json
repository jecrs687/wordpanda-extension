{ "name": "LinguaBoost",
  "description": "LinguaBoost",
  "version": "1.1",
  "manifest_version": 3,
  "content_security_policy": "'self' 'unsafe-inline'",
  "permissions": [],
  "host_permissions": [],

  "content_scripts": [
    {
        "matches": ["https://www.primevideo.com/*", "https://primevideo.com/*"],
        "js": ["./background/back.js"]
    }],
 "web_accessible_resources": [
        {"resources":
        ["./background/background.js"],
        "matches": ["https://www.primevideo.com/*", "https://primevideo.com/*"]

      }
    ],
  "action": {
    "default_title": "LinguaBoost",
    "default_icon": "react-structure/public/logo192.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "react-structure/public/logo192.png"
  }
}
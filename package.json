{
  "name": "languageboost",
  "private": true,
  "version": "0.0.64",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build; cp manifest.json dist/; cp -r jobs dist/; cp -r icons dist/; cp -r scripts dist/",
    "build:auto": "npx nodemon --exec \"yarn build ; node pipe\"",
    "server": "serve dist --listen 8080",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build-extension": "yarn build; rm -rf dist.zip & zip -r dist.zip dist"
  },
  "dependencies": {
    "@types/chrome": "^0.0.266",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsc": "^2.0.4",
    "zustand": "^4.4.7"
  },
  "nodemonConfig": {
    "ignore": [
      "**/test/**",
      "**/docs/**",
      "**/node_modules/**",
      "**/dist/**"
    ],
    "ext": "js, ts",
    "delay": 200
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "serve": "^14.2.1",
    "typescript": "^5.0.2",
    "vite": "^5.2.8"
  }
}